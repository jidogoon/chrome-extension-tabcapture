{
  "name": "Chrome Tab Capture Example",
  "description": "",
  "version": "1",
  "manifest_version": 2,
  "background": {
    "scripts": ["eventPage.js"],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
    "storage",
    "tabs",
    "tabCapture"
  ]
}
